{
  "name": "Improved UTOL",
  "description": "Extension for UTokyo LMS on Google Chrome",
  "version": "3.0.1",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["https://utol.ecc.u-tokyo.ac.jp/lms/timetable*"],
      "js": ["src/main.js"],
      "css": ["style/tasklist.css", "style/loader.css"],
      "run_at": "document_idle"
    }
  ]
}
